{
  "Loggers": {
    "FileLogger": {
      "Path": "logs\\log-{Date}.txt",
      "Enabled": true,
      "MinLevel": "Information"
    },
    "ConsoleLogger": {
      "Template": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}",
      "Enabled": true,
      "MinLevel": "Verbose"
    },
    "ApplicationInsightsLogger": {
      "InstrumentationKey": "{{appInsightsKey}}",
      "Enabled": true,
      "MinLevel": "Warning"
    }
  },
  "SqlServer": {
    "ConnectionString": ""
  },
  "ServiceBus": {
    "enabled": true,
    "connectionString": ""
  }
}
